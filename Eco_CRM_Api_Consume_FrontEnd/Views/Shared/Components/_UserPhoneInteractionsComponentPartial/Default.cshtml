@model List<DisplayUserPhoneInteractionCountResponse>


<div class="info-box">
    <h4>En Çok Telefonla Aranan</h4>
    <ul id="mostCalledList">
        @foreach (var x in Model)
        {
            <li>
                <a href="/CustomerOperation/OperationsByCustomer/@x.CustomerId">@x.CompanyName</a>
                <span>@x.InteractionCount</span>
            </li>

        }
    </ul>
</div>